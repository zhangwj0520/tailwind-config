{
  "name": "tailwindcss",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "changeset&&changeset version",
    "pb": "changeset publish",
    "sync": "pnpx tsx ./scripts/sync.ts",
    "sync-cestc": "pnpm tsx ./scripts/sync-cestc.ts",
    "release1": "bumpp -r"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@tailwindcss/typography": "^0.5.9",
    "@zhangwj0520/eslint-config-ts": "latest",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "prettier": "latest",
    "tailwindcss": "^3.2.7",
    "turbo": "latest",
    "typescript": "~4.8.4",
    "zx": "^7.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.28.0"
}
